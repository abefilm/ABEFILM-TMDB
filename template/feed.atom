    <title>Video</title>
    <content type='html'>&lt;div style="max-width:100%;background:#000;"&gt;
  &lt;video id="player"
    controls="controls"
    autoplay="autoplay"
    playsinline="playsinline"
    crossorigin="anonymous"
    style="width:100%;max-height:100vh;background:#000;object-fit:contain;"&gt;
  &lt;/video&gt;
&lt;/div&gt;

&lt;script src="https://cdn.jsdelivr.net/npm/hls.js@latest"&gt;&lt;/script&gt;
&lt;script&gt;
  function getQueryParam(name) {
    const urlParams = new URLSearchParams(window.location.search);
    return urlParams.get(name);
  }

  document.addEventListener('DOMContentLoaded', function () {
    const video = document.getElementById('player');
    const url = getQueryParam("src");

    if (!url) {
      video.outerHTML = "&lt;p style='color:white;text-align:center'&gt;No video source provided. Use ?src=yourfile.m3u8&lt;/p&gt;";
      return;
    }

    if (Hls.isSupported()) {
      const hls = new Hls();
      hls.loadSource(url);
      hls.attachMedia(video);
    } else if (video.canPlayType('application/vnd.apple.mpegurl')) {
      video.src = url; // Safari
    } else {
      video.outerHTML = "&lt;p style='color:white;text-align:center'&gt;Your browser does not support HLS playback.&lt;/p&gt;";
    }
  });
&lt;/script&gt;
</content>
